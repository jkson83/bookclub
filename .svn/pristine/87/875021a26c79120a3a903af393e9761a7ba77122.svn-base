  border: 1px solid #C6C6C6;
  border-radius: 3rem;
}
.section-m-area.line .form-s-content {
  padding-top: 3.2rem;
}
.section-m-box h2 {
  color: #111;
  font-family: "binggrae";
  font-size: 2.4rem;
  font-weight: 700;
  letter-spacing: -0.03rem;
  text-align: center;
}
.section-m-box .btn-wrap .btn.primary {
  font-size: 1.8rem;
  font-weight: 700;
}
.section-m-content {
  padding-top: 2.3rem;
}
.section-m-item {
