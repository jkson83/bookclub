*{
	margin:0;
	padding:0;
	box-sizing: border-box;
}
html{
	background-color:#000;
}
body{
	overflow:hidden;
	margin:0;
	padding:0;
	height:100%;
	background:none;
}

.magazine{
	display:inline-block;
	position: fixed;
	top:10.86vh;
	left:50%;
	transform:translate(-50%,0);
	max-height:100vh;
	&.double{
		overflow: visible !important;
		.page{
			img{
				max-width: 100%;
			}
		}
	}
	&.zoom{
		.play{
			&-top1{
				top: 5%;
				left: 12%;
			}
			&-top2{
				top: 5%;
				right: 12%;
			}
			&-center1{
				top: 64%;
				left: 12%;
			}
			&-center2{
				top: 64%;
				right: 12%;
			}
			&-bottom1{
				bottom: 10%;
				left: 12%;
			}
			&-bottom2{
				bottom: 10%;
				right: 12%;
			}
		}
	}
}

.magazine .page{
	display:flex;
	justify-content: flex-start;
	align-items: center;
	background-color: #fff;

	&.even{
		justify-content: flex-end;
	}
}

.magazine .p-temporal{
	height:100% !important;
}

.magazine .page img{
	width:auto;
	max-height:100%;
}

.thumbnails {
	display: flex;
	position: fixed;
	bottom:-50px;
	left:20px;
	right:20px;
	height:0;
	padding:10px;
	z-index: 10;
	border-radius: 10px 10px 0 0;
	background-color: #000;
}
.btn_thumbnail {
	position:fixed;
	top:auto;
	bottom:0;
	left:50%;
	z-index: 11;
	width: 100px;
	height: 20px;
	border-radius:0 0 10px 10px;
	box-shadow:0 0 0 1px #fff;
	border-bottom:0;
	transform: translateX(-50%) rotate(180deg);
	background:#000 url("data:image/svg+xml,%3C%3Fxml version='1.0' standalone='no'%3F%3E%3C!DOCTYPE svg PUBLIC '-//W3C//DTD SVG 20010904//EN' 'http://www.w3.org/TR/2001/REC-SVG-20010904/DTD/svg10.dtd'%3E%3Csvg version='1.0' xmlns='http://www.w3.org/2000/svg' width='1070.000000pt' height='1280.000000pt' viewBox='0 0 1070.000000 1280.000000' preserveAspectRatio='xMidYMid meet'%3E%3Cmetadata%3E%0ACreated by potrace 1.15, written by Peter Selinger 2001-2017%0A%3C/metadata%3E%3Cg transform='translate(0.000000,1280.000000) scale(0.100000,-0.100000)'%0Afill='%23ffffff' stroke='none'%3E%3Cpath d='M4062 8810 c2 -2195 1 -3990 -2 -3990 -3 0 -917 912 -2032 2027%0Al-2028 2028 0 -1765 0 -1765 2673 -2673 2672 -2672 2673 2673 2673 2673 6%0A1769 5 1770 -2021 -2020 -2021 -2020 0 3977 0 3978 -1300 0 -1300 0 2 -3990z'/%3E%3C/g%3E%3C/svg%3E%0A") no-repeat 50% 50%;
	background-size: 10px auto;
}
.btn_thumbnail.active{
	bottom:150px;
	border-radius:10px 10px 0 0;
	box-shadow:0px -2px 6px -2px #fff;
	transform: translateX(-50%) rotate(0deg);
}
.btn_thumbnail.active ~ .thumbnails{
	bottom:0;
	height:auto;
	box-shadow:0px 0px 6px 0px #fff;
}
.thumbnails {
	.swiper-slide{
		width:auto;
		cursor: pointer;
		img{
			width:auto;
			height: 130px;
		}
	}
	.swiper-button-next,.swiper-button-prev{
		position: absolute;
		top:50%;
		z-index: 111;
		width: 50px;
		height: 50px;
		margin-top:-25px;
		cursor: pointer;
		&.swiper-button-disabled{
			display: none;
		}
	}
	.swiper-button-next{
		right:0;
		background-image: url("data:image/svg+xml,%3C%3Fxml version='1.0' %3F%3E%3Csvg data-name='Your Icons' id='Your_Icons' viewBox='0 0 96 96' xmlns='http://www.w3.org/2000/svg'%3E%3Cdefs%3E%3Cstyle%3E.cls-1%7Bfill:%23336dff;%7D%3C/style%3E%3C/defs%3E%3Cpath class='cls-1' d='M48,90A42,42,0,1,1,90,48,42,42,0,0,1,48,90Zm0-80A38,38,0,1,0,86,48,38,38,0,0,0,48,10Z'/%3E%3Cpath class='cls-1' d='M38,70a2,2,0,0,1-1.41-3.41L55.17,48,36.59,29.41a2,2,0,0,1,2.82-2.82l20,20a2,2,0,0,1,0,2.82l-20,20A2,2,0,0,1,38,70Z'/%3E%3C/svg%3E");
	}
	.swiper-button-prev{
		left:0;
		background-image: url("data:image/svg+xml,%3C%3Fxml version='1.0' %3F%3E%3Csvg data-name='Your Icons' id='Your_Icons' viewBox='0 0 96 96' xmlns='http://www.w3.org/2000/svg'%3E%3Cdefs%3E%3Cstyle%3E.cls-1%7Bfill:%23336dff;%7D%3C/style%3E%3C/defs%3E%3Cpath class='cls-1' d='M48,90A42,42,0,1,1,90,48,42,42,0,0,1,48,90Zm0-80A38,38,0,1,0,86,48,38,38,0,0,0,48,10Z'/%3E%3Cpath class='cls-1' d='M38,70a2,2,0,0,1-1.41-3.41L55.17,48,36.59,29.41a2,2,0,0,1,2.82-2.82l20,20a2,2,0,0,1,0,2.82l-20,20A2,2,0,0,1,38,70Z'/%3E%3C/svg%3E");
		transform: rotate(180deg);
	}
}

button{
	position: absolute;
	top:50%;
	z-index: 111;
	width:30px;
	height:30px;
	border:none;
	font-size:0;
	border-radius:50%;
	background:none;
	background-color:rgba(255, 255, 255, 0.5);
	background-size:100% auto;
	transform:translateY(-50%);
	background-image: url("data:image/svg+xml,%3C%3Fxml version='1.0' %3F%3E%3Csvg data-name='Your Icons' id='Your_Icons' viewBox='0 0 96 96' xmlns='http://www.w3.org/2000/svg'%3E%3Cdefs%3E%3Cstyle%3E.cls-1%7Bfill:%23336dff;%7D%3C/style%3E%3C/defs%3E%3Cpath class='cls-1' d='M48,90A42,42,0,1,1,90,48,42,42,0,0,1,48,90Zm0-80A38,38,0,1,0,86,48,38,38,0,0,0,48,10Z'/%3E%3Cpath class='cls-1' d='M38,70a2,2,0,0,1-1.41-3.41L55.17,48,36.59,29.41a2,2,0,0,1,2.82-2.82l20,20a2,2,0,0,1,0,2.82l-20,20A2,2,0,0,1,38,70Z'/%3E%3C/svg%3E");

	&::before{
		content: "";
		width: 3.22vh;
		height: 4.98vh;
		transform: translateY(-50%);
		position: absolute; 
		top: 50%;
	}
}
.prevBtn {
	left:0;
	transform: translate(-50%, -50%);
	position: absolute;

	&::before{
		background-image: url("data:image/svg+xml,%3Csvg viewBox='0 0 34 52' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Crect width='35.5361' height='11.8454' rx='5.92268' transform='matrix(0.696475 -0.717581 0.696475 0.717581 0.5 26)' fill='%2309A0FF'/%3E%3Crect width='35.5361' height='11.8454' rx='5.92268' transform='matrix(0.696475 0.717581 0.696475 -0.717581 0.5 26)' fill='%2309A0FF'/%3E%3C/svg%3E");
		background-size: 100% 100%;
		right: 3.12vh; 
	}
}
.nextBtn {
	right:0;
	transform: translate(50%, -50%);

	&::before{
		background-image: url("data:image/svg+xml,%3Csvg viewBox='0 0 34 52' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Crect width='35.5361' height='11.8454' rx='5.92268' transform='matrix(0.696475 -0.717581 0.696475 0.717581 0.5 26)' fill='%2309A0FF'/%3E%3Crect width='35.5361' height='11.8454' rx='5.92268' transform='matrix(0.696475 0.717581 0.696475 -0.717581 0.5 26)' fill='%2309A0FF'/%3E%3C/svg%3E");
		background-size: 100% 100%;
		transform: translateY(-50%) rotateY(180deg);
		left: 3.12vh; 
	}
}
.play{
	position: absolute;
	z-index: 111;
	width: 70px;
	height: 70px;
	margin:0;
	font-size:0;
	border-radius:50%;
	transform:translate(0);
	background-position:50% 50%;
	background-repeat:no-repeat;
	background-image: url("data:image/svg+xml,%3Csvg viewBox='0 0 40 26' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath fill-rule='evenodd' clip-rule='evenodd' d='M6.34921 5.21954C2.84264 5.21954 2.01977e-07 8.06218 1.60162e-07 11.5687L1.25198e-07 14.5008C8.33823e-08 18.0073 2.84264 20.85 6.34921 20.85L11.1258 20.85L11.1258 20.8494L20.5066 24.7527C22.5975 25.6227 24.9008 24.0864 24.9008 21.8217L24.9008 4.17485C24.9008 1.92961 22.6337 0.393898 20.5486 1.22672L10.552 5.21954L6.34921 5.21954Z' fill='%23777777'/%3E%3Cpath fill-rule='evenodd' clip-rule='evenodd' d='M30.1555 11.9266C29.8596 10.4554 29.0916 9.15719 28.2738 8.19779C27.7835 7.62245 27.7107 6.75738 28.2084 6.1884C28.6921 5.63543 29.5346 5.56762 30.0346 6.10595C31.219 7.38115 32.3946 9.21855 32.8352 11.4092C33.3571 14.0041 32.8159 16.9521 30.336 19.837C29.8564 20.395 29.0088 20.3908 28.4833 19.876C27.9424 19.346 27.9516 18.4806 28.4309 17.8943C30.238 15.684 30.4974 13.6269 30.1555 11.9266Z' fill='%23777777'/%3E%3Cpath fill-rule='evenodd' clip-rule='evenodd' d='M37.0308 10.9154C36.3827 7.68838 34.5392 4.93884 32.7583 3.12396C32.229 2.58456 32.166 1.71784 32.6722 1.15667C33.1541 0.622389 33.9757 0.562835 34.4892 1.0668C36.6333 3.17095 38.9108 6.45264 39.7065 10.4144C40.586 14.7937 39.6227 19.8612 34.9875 24.871C34.4976 25.4005 33.6713 25.4051 33.1485 24.9082C32.599 24.3861 32.5967 23.5143 33.1061 22.953C37.0659 18.5891 37.7318 14.4055 37.0308 10.9154Z' fill='%23777777'/%3E%3C/svg%3E%0A");
	background-size:40px auto;
	background-color:rgba(255,255,255,0.5);
	box-shadow: 0px -2.9px 10px 0px #99999952 inset, 0px 3.1px 4.7px 0px #FFFFFF40 inset, 0px 6.3px 12.7px 0px #976B4B29;
	transition:transform .3s ease-in-out;

	&-top1{
		top: 5%;
		left: 5%;
	}

	&-top2{
		top: 5%;
		right: 5%;
	}

	&-center1{
		top: 64%;
		left: 5%;
	}

	&-center2{
		top: 64%;
		right: 5%;
	}

	&-bottom1{
		bottom: 10%;
		left: 5%;
	}

	&-bottom2{
		bottom: 10%;
		right: 5%;
	}
}
.play.active{
	transform: scale(1.2);
  // animation-duration: 0.5s;
  // animation-name: play;
  // animation-iteration-count: infinite;
}
.play.left{
	left:150px;
	top:150px;
}
.wrap{
	overflow: hidden;
	height:100%;
	width:100vw;
}
.utill{
	display:flex;
	position: fixed;
	left:50%;
	top:-40px;
	width:130px;
	padding:0 0 10px;
	border-radius:0 0 10px 10px;
	transform: translateX(-50%);
	background-color:#000;
	justify-content:space-evenly;
	&.active{
		top:0;
		box-shadow: 0 1px 0 1px #fff;
	}
	.btn_utill.btn_thumbnail.active{
		top:40px;
		transform:translateX(-50%) rotate(180deg);
		box-shadow:0 -1px 0 1px #fff;
	}
}
.btn_utill.btn_thumbnail{
	top:40px;
	bottom:auto;
	transform:translateX(-50%) rotate(0deg);
	box-shadow: 0 1px 0 1px #fff;
}

