				// 	}
				}
				&-highlight-inner{
					position: relative;
					width: 100%;
					height: 56vh;
					padding-left: 1vw;
					overflow-y: auto;
					scroll-behavior: smooth;
					
					&.scroll{
						&::-webkit-scrollbar {
							width: 8px;
						}
						&::-webkit-scrollbar-thumb {
							background-color: #eee;
							border-radius:20px;
						}
						&::-webkit-scrollbar-track {
							background-color: #fff;
						}
					}
					
					& > span {
						position: relative;
						z-index: 0;
						
						color: #000;
						font-size: 3.5vh;
						font-family: "Binggrae";
						font-weight: 700;
						line-height: 1.6;
						letter-spacing: -0.2vh;
					}

					& > span > i {
						position: absolute;
						left: 0;
						top: 0;
						z-index: -1;
						width: 100%;
						height: 100%;
					}

					& > span.active > i {
						background-color: #FFCB14;
						animation-name: myHighlight;
						animation-timing-function: linear;
					}

					@keyframes myHighlight {
						0% {
							width: 0%;
						}

						100% {
							width: 100%;
						}
					}
				}
				&-img-box{
					position: relative;
					width: 30%;
					height: 100%;
					z-index: 3;
					display: flex;
					align-items: center;
					justify-content: center;
