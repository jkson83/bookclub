@keyframes outerglow {
	0% {
		filter: drop-shadow(0px 0 0 #FAFF00);
	}
	50% {
		outline: 2px solid #FAFF00;
		filter: drop-shadow(0px 10px 20px #FAFF00);
	}
	100%{
		filter: drop-shadow(0px 0 0 #FAFF00);
	}
}

/* 로그인, 회원가입 등 */
.info-txt{
	margin: .5rem 0 1rem;
	text-align: right;
	font-family: "pretendard";
	font-size: 0.75rem;
	font-weight: 400;
	letter-spacing: -0.015rem;

	&:last-of-type{
		margin-bottom: 0;
	}
	&.error{
		color:#E51F33;
	}
}

.bx-line{
	overflow: hidden;
	min-height: 10.25rem;
	max-height: 16rem;
	display: flex;
	justify-content: center;
	flex-direction: column;
	align-items: center;
	text-align: center;
	border: 1px solid #C6C6C6;
	border-radius: .625rem;

	&.scroll{
		.inner{
			width: 100%;
			overflow-y: overlay;
			padding: 1.25rem .5rem 1.25rem 1.25rem;
			text-align: left;
			scrollbar-width: thin;
			scrollbar-color: black white;

			&::-webkit-scrollbar{
				width: 12px;
			} 
			&::-webkit-scrollbar-thumb{
				background: #D9D9D9;
				border: 2px solid #fff;
				border-radius: 12px;
			}
			::-webkit-scrollbar-thumb:hover {
				background-color:#a0a0a5;
			}
			::-webkit-scrollbar-track{
				background-color: rgba(255, 255, 255, 0.8);
			}
		}
	}
	&.result{
		strong{
			color: #111;
			font-family: 'pretendard';
			font-size: 1.25rem;
			line-height: 1.75rem;
			letter-spacing: -.025rem;
		}
		p {
			margin-top: .5rem;
			color: #000;
			font-family: 'pretendard';
			font-size: .875rem;
			letter-spacing: -.0175rem;

			span{
				font-family: inherit;
				font-weight: 700;
			}
		}
	}
}

.term-text{
	& + .term-text{
		margin-top: 2vh;
	}
}

.term-text-list{
	margin-top: 2vh;

	dt{
		font-weight: 700;
	}

	dd{
		margin-top: 0.5vh;
	}

	& + .title-term{
		margin-top: 2vh;
	}
}

.term-date{
	margin-top: 2vh;
}

.term-text2{
	margin-top: 2vh;
}

.title-term{
	font-size: 1.92vh;
	font-weight: 700;
}

@counter-style circled-numbers {
    system: fixed;
    symbols: "\2460" "\2461" "\2462" "\2463" "\2464" "\2465" "\2466" "\2467" "\2468" "\2469";
}

.term-text-round{
	margin-left: 1.3vw;
	counter-reset: circle-counter;

	> li{
		counter-increment: circle-counter;

		&::marker {
			content: counter(circle-counter, circled-numbers) " ";
		}
	}
}

.term-text-sub{
	margin-left: 1.3vw;

	li{
		list-style: decimal;
	}
}

.paging{
	margin-top: 2.5rem;

	ul{
		display: flex;
		justify-content: center;

		li{margin: 0 .12rem;}
	}
	a{
		display: flex;
		align-items: center;
		justify-content: center;
		width: 2.625rem;
		height: 2.625rem;
		color: #fff;
		font-family: 'pretendard';
		font-size: 1.5rem;
		line-height: 1;
		background-color: #C6C6C6;

		&:hover, &:active, &:focus, &.current{
			background-color: #111;
		}
	}
	button{
		position: relative;
		width: 2.625rem;
		height: 2.625rem;

		&.prev::before, &.next::before{
			content:'';
			position: absolute;
			left: 50%;
			top: 50%;
			display: inline-block;
			width: 1rem;
			height: 1rem;
			background: url('../../resources/img/app/ico_arrow_prev.png') 50% 50% no-repeat;
			background-size: 1rem; 
			transform: translate(-50%, -50%);
		}
		&.next::before{
			transform: translate(-50%, -50%) rotate(180deg);
		}
	}
}

.hidden { display:none;}

