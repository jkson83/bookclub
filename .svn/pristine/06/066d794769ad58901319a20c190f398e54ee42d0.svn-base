						transform: rotateY(0);
						background: #fff;
						cursor: default;
						animation-name: fadeIn;
						animation-duration: 0.3s;
					}

					&.show {
						font-size: 33px;
						pointer-events: none;

						&::after{
							opacity: 0;
						}

						.card-img-area{
							opacity: 1;
						}
					}

					&.match {
						cursor: default;
						font-size: 33px;
						animation-duration: 0.3s;
						animation-name: bounceIn;
						opacity: 0;
					}

					&.shake {
						animation-duration: 0.5s;
						animation-name: shake;
					}
				}

				&.deck-2x2{
					width: 41.46vh;
					height: 46.5vh;
				}
			}
			
			/*
			 * Styles for the Score Panel
			 */
			
			.score-panel {
				display: flex;
				justify-content: space-between;
				align-items: center;
				text-align: left;
				position: absolute;
				left: 0; 
				top: 0;
				bottom: 0;

				.movesCounter {
					display: none;
				}

				.timer {
					width: 12.11vh;
					height: 12.11vh;
					padding-top: 7.21vh;
					background-color: #fff;
					box-shadow: -2px -2px 4px 0px #FFFFFF40 inset, 6px 6px 4px 0px #FF8A0040, -6px -6px 4px 0px #08B5FF1A inset;
					border-radius: 100%;
					font-family: 'Binggrae';
					font-size: 2.69vh;
					color: #FF5C00;
					font-weight: 700;
					text-align: center;
					line-height: 1;
					transform: translateY(-50%);
					position: absolute;
					left: 7.97vw; 
					top: 50%;

					&::before{
						content: "";
						width: 2.88vh;
						height: 4.01vh;
						background-image: url($icon-clock);
						background-size: 100% 100%;
						transform: translateX(-50%);
						position: absolute;
						left: 50%; 
						top: 2.01vh;
					}
				}

				.restart {
					display: none;
				}
			}

			/*
			* Animations
			*/
			@keyframes bounceIn {
				from,
				20%,
				40%,
				60%,
				80%,
				to {
					animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
				}

				0% {
					opacity: 0;
					transform: scale3d(0.3, 0.3, 0.3);
				}

				20% {
					transform: scale3d(1.1, 1.1, 1.1);
				}

				40% {
					transform: scale3d(0.9, 0.9, 0.9);
				}

				60% {
					opacity: 1;
					transform: scale3d(1.03, 1.03, 1.03);
				}

				80% {
					transform: scale3d(0.97, 0.97, 0.97);
				}

				to {
					opacity: 1;
					transform: scale3d(1, 1, 1);
				}
			}

			@keyframes hue-animation {
				0% {
					filter: hue-rotate(80deg);
				}
				20% {
					filter: hue-rotate(150deg);
				}
				40% {
					filter: hue-rotate(210deg);
				}
				60% {
					filter: hue-rotate(180deg);
				}
				60% {
					filter: hue-rotate(90deg);
				}
				60% {
					filter: hue-rotate(50deg);
				}
			}

			@keyframes shake {
				from,
				to {
					transform: translate3d(0, 0, 0);
				}

				10%,
				30%,
				50%,
				70%,
				90% {
					transform: translate3d(-10px, 0, 0);
				}

				20%,
				40%,
				60%,
				80% {
					transform: translate3d(10px, 0, 0);
				}
			}

			@keyframes fadeIn {
				from {
					opacity: 0;
				}

				to {
					opacity: 1;
				}
			}
		}
	}

