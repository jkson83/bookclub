@charset "utf-8";



.txt-blue{color: #09A0FF !important;}
.txt-l{text-align: left !important;}

[data-pagescroll]{
	position: relative;
	overflow: auto;
	height: 100vh;
}
[data-pagescroll=hidden] {
	overflow: hidden;
}
.question-box{
	display: flex;
	justify-content: center;
	align-items: center;
	width: 125.73vh;
	width: 129vh;
	padding: 0 10%;
	margin: 3.418vh auto 0;
	font-family: 'Binggrae';
	font-size: 3.71vh;
	color: #fff;
	font-weight: 700;
	text-align: center;
	line-height: 1.3;
	position: relative;

	&.line-3{ 
		height: 31.25vh;
		background: url("../../resources/img/app/bg_q_line3.png") no-repeat center center;
		background-size: contain;
	}

	&.line-2{
		height: 20.16vh;
		background: url("../../resources/img/app/bg_q_line2.png") no-repeat center center;
		background-size: contain;
	}

	&.line-2-2{
		height: 24.11vh;
		background: url("../../resources/img/app/bg_q_line2_2.png") no-repeat center center;
		background-size: contain;
	}

	.q-sound{
		display: flex;
		align-items: center;
		justify-content: center;
		width: 6.83vh;
		height: 6.83vh;
		background-color: #CB7031;
		box-shadow: 0px -2.8px 4.2px 0px #14102780 inset, 0px 3.1px 4.7px 0px #FFFFFF40 inset, 0px 6.3px 12.6px 0px #00000029;
		transform: translateY(0);
		position: absolute;
		// right: 5.88vh;
		right: 7.88vh;
		top: 3.9vh;
		font-size: 0;
		@include text-hide;

		&::before{
			content: "";
			display: block;
			width: 3.9vh;
			height: 2.53vh;
			background-image: url($btn-sound);
		}
	}

	.question-drag-text{
		font-family: 'Binggrae';
		font-size: 3.71vh;
		color: #fff;
		font-weight: 700;
		text-align: center;
		line-height: 1.3;

		.drop-zone{
			display: inline-block;
			// width: 24.45vw;
			min-width: 8vw;
			height: 5.85vh;
			padding:0 2vw;
			background-color: #fff;
			border: 5px solid #4E270B;
			border-radius: 30px;
			font-family: 'Binggrae';
			font-size: 3.84vh;
			color: #09A0FF;
			font-weight: 700;
			text-align: center;
			line-height: 4.5vh;
			vertical-align: middle;

			img{
				width: auto;
				height: 5.85vh;
			}
		}

		.text-length-m {
			width:26vh;
		}

		.text-length-l {
			width:30vh;
		}

	}
}

.word-meaning{
	font-family: 'Binggrae';
	font-size: 3.84vh;
	color: #A84700;
	font-weight: 700;
	text-align: center;
	line-height: 1.5;
	word-break:keep-all;

	.word-box{
		display: inline-block;
		height: 5.85vh;
		padding: 0 2.21vw;
		background-color: #fff;
		border: 5px solid #FFB800;
		border-radius: 30px;
		font-family: 'Binggrae';
		font-size: 3.84vh;
		color: #FF6B00;
		font-weight: 700;
		text-align: center;
		// line-height: 5vh;
		line-height: 1.17;
		vertical-align: middle;
	}
}

.wrap{
	display: flex;
	height: 100vh;
	justify-content: center;
	align-items: center;
	flex-direction: column;

	//스플래쉬
	&.splash{
		background-color: #fff;

		.splash-text{
			font-family: 'Binggrae'; 
			text-align:center;
			font-weight: 700;
			font-size: 4.37vh;
			line-height: 1;

			p{
				margin-top: 2.5vh;
				font-family: 'Binggrae'; 
				font-size: 6.67vh;
				font-weight: 700;
				line-height: 1;
			}
		}

		.splash-logo{
			margin-top: 7.4vh;
			text-align: center;

			.move-img{
				img{
					width: auto;
					height: 23.17vh;
				}
			}

			.logo-text{
				margin-top: 2.69vh;

				img{
					width: auto;
					height: 6.34vh;
				}
			}			
		}

		.f-splash-text{
			margin-top: 7.5vh;
			font-size: 2.5vh;
		}
	}

	.text-login{
		font-family: 'Binggrae'; 
		color: #fff;
		text-align:center;
		font-weight: 700;
		font-size: 2.27vw;
		line-height: 1;

		p{
			margin-top: 1.257vh;
			margin-bottom: 2.2vh;
			font-family: 'Binggrae'; 
			font-size: 3.98vw;
			font-weight: 700;
			line-height: 1;
		}
	}

	.everybooks{
		text-shadow: calc(3px * 1) 0 0 #fff, calc(3px * 0.9239) calc(3px * 0.3827) 0 #fff, calc(3px * 0.7071) calc(3px * 0.7071) 0 #fff,
		calc(3px * 0.3827) calc(3px * 0.9239) 0 #fff, 0 calc(3px * 1) 0 #fff, calc(3px * -0.3827) calc(3px * 0.9239) 0 #fff, 
		calc(3px * -0.7071) calc(3px * 0.7071) 0 #fff, calc(3px * -0.9239) calc(3px * 0.3827) 0 #fff, calc(3px * -1) 0 0 #fff,
		calc(3px * -0.9239) calc(3px * -0.3827) 0 #fff, calc(3px * -0.7071) calc(3px * -0.7071) 0 #fff, calc(3px * -0.3827) calc(3px * -0.9239) 0 #fff,
		0 calc(3px * -1) 0 #fff, calc(3px * 0.3827) calc(3px * -0.9239) 0 #fff, calc(3px * 0.7071) calc(3px * -0.7071) 0 #fff,
		calc(3px * 0.9239) calc(3px * -0.3827) 0 #fff;

		span{
			font-family: 'Binggrae'; 
			font-size: 3.98vw;
			font-weight: 900;

			&.text-blue{
				color: #09A0FF;
			}
			&.text-green{
				color: #1F9F3C;
			}
			&.text-yellow{
				color: #F1C200;
			}
			&.text-red{
				color: #DE0927;
			}
			&.text-black{
				color: #1D1D1B;
			}
		}
	}

	//로그인
	&.login{
		display: flex;
		height: 100vh;
		background: url("../../resources/img/app/bg_login.png") no-repeat center center;
		background-size: cover;
		overflow-y: auto;

		.login-box{
			width: 52.7%;
			padding: 3.29vw 3.26vw;
			margin-top: 4.7vh;
			background-color: #fff;
			box-shadow: 0px -3.6px 5.4px 0px #14102780 inset, 0px 4px 6px 0px #FFFFFF40 inset, 0px 8px 16px 0px #00000029;
			border-radius: 60px;

			.login-input-area{
				position:relative;

				label{
					display: block;
					font-size: 1.76vw;
					font-weight: 700;
					position: absolute;
					left: calc(1.72vw + 4px);
					top: 1.78vh;
				}
					
				input{
					width:100%;
					height: 6.08vw;
					padding-top: 3.43vw;
					padding-left: 1.72vw; 
					padding-bottom: 1.3vw;
					border:4px solid #CFCFCF;
					border-radius: 20px;
					font-size: 1.46vw;
					line-height: 1.64;

					&:focus{
						border-color: #111;
					}

					&::placeholder{
						color: #888;
					}
				}

				& + .login-input-area{
					margin-top: 1vw;
				}
			}

			.login-option-area{
				display: flex;
				margin-top: 1.61vw;
				justify-content: space-between;
				align-items: center;

				.login-option-list{
					display: flex;
					gap: 1.39vw;
				}

				.search-id-pw{
					border-bottom: 1px solid #000;
					font-size: 1.17vw;
					color: #000;
					font-weight: 700;

					a{
						font-size: 1.17vw;
						color: #000;
						font-weight: 700;
					}
				}
			}

			.btn-login-area{
				margin-top: 2.81vh;
			
				button{
					width: 100%;
					height: 5.58vw;
					background-color: #ED4043;
					box-shadow: 0px -3.6px 5.4px 0px #14102780 inset, 0px 4px 6px 0px #FFFFFF40 inset, 0px 8px 16px 0px #00000029;
					border-radius: 100px;
					font-family: 'Binggrae'; 
					font-size: 1.84vw;
					color: #fff;
					font-weight: 700;
					transform: translateY(0);
					position: static;
				}
			}
		}

		.join-btn-area{
			margin-top: 2.19vw;
			font-size: 1.17vw;
			color: #fff;
			text-align: center;

			a{
				margin-left: 0.87vw;
				border-bottom: 1px solid #fff;
				font-size: 1.17vw;
				color: #fff;
			}
		}
	}

	//프로필 선택
	&.profile-select{
		background: url("../../resources/img/app/bg_profile.png") no-repeat center center;
		background-size: cover;

		.text-together{
			font-family: 'Binggrae'; 
			font-size: 6.15vh;
			font-weight: 700;
			line-height: 1.3;
			color: #fff;
			text-align: center;
		}

		.profile-select-area{
			display: flex;
			margin-top: 5.76vh;
			justify-content: center;
			gap: 7.32vw;
		}

		.profile-btn{
			display: block;
			width: 19.23vh;
			height: auto;
			flex-shrink: 0;
			transform: translateY(0);
			position: relative;
			top: auto;
			
			img{
				width: 19.23vh;
				height: 19.23vh;
				border-radius: 100%;
			}

			.profile-img{
				display: block;
				background-color: #ffffff50;
				border-radius: 100%;
			}

			.text-step{
				display: inline-block;
				height: 3.84vh;
				padding: 0 1.31vw;
				background-color: #ED4043;
				border-radius: 100px;
				box-shadow: 0px -3.6px 5.4px 0px #14102780 inset, 0px 4px 6px 0px #FFFFFF40 inset, 0px 8px 16px 0px #00000029;
				font-family: 'Binggrae'; 
				font-size: 1.53vh;
				color: #fff;
				font-weight: 700;
				text-align: center; 
				line-height: 3.84vh;
				white-space: nowrap;
				transform: translateX(-50%);
				position: absolute;
				left: 50%;
				top: 17vh; 
			}

			.text-name{
				display: block;
				margin-top: 2.69vh;
				font-family: 'Binggrae'; 
				font-size: 4vh;
				font-weight: 700; 
				text-align: center;
			}
		}
	}

	//책읽기
	&.read{
		justify-content: flex-start;
		background-color: #111;

		.read-top{
			width: 100%;
			height: 11vh;
			position: relative;

			.title-book{
				font-family: 'Binggrae'; 
				font-size: 3.7vh;
				color: #fff;
				line-height: 11vh;
				font-weight: 700;
				text-align: center;
			}

			.read-sticker-area{
				display: flex;
				gap: 0.73vw;
				transform: translateY(-50%);
				position: absolute;
				left: 4.39%; 
				top: 50%;
			}

			.btn-read{
				display: flex;
				justify-content: center;
				align-items: center;
				gap: 0.58vw;
				width: 16.83vw;
				height: 6.83vh;
				border-radius: 100px;
				font-family: 'Binggrae';
				font-size: 2.73vh;
				color: #fff;
				text-align: left;
				font-weight: 700;
				transform: translateY(0);
				position: absolute;
				right: 4.39%; 
				top: 2.14vh;

				&::after{
					content: "";
					display: block;
				}

				&.read-stop{
					background-color: #ED4043;
					box-shadow: 0px -3.6px 5.4px 0px #14102780 inset, 0px 4px 6px 0px #FFFFFF40 inset, 0px 8px 16px 0px #00000029;

					&::after{
						width: 2.4vh;
						height: 2.4vh;
						background: url($btn-x) no-repeat center center;
						background-size: 100% 100%;
					}
				}

				&.read-complete{
					background-color: #09A0FF;
					box-shadow: 0px -3.6px 5.4px 0px #14102780 inset, 0px 4px 6px 0px #FFFFFF40 inset, 0px 8px 16px 0px #00000029;

					&::after{
						width: 2.63vh;
						height: 1.95vh;
						background: url($btn-complete) no-repeat center center;
						background-size: 100% 100%;
					}
				}
			}
		}

		.book-content{
			display: flex;
			justify-content: center;
			width: 100%;
			height: 83.75vh;
			padding:0;
			position: relative;

			.read-book-img{
				display: inline-block;
				//width: 100%;
				position: relative;
				overflow: hidden;
				
				img{
					width: auto;
				}
			}

			.book-page{
				width: 7.1vw;
				height: 3.9vh;
				background-color: #ffffff60;
				border-radius: 100px;
				font-size: 1.56vh;
				line-height: 3.9vh;
				text-align: center;
				transform: translateX(-50%);
				position: absolute;
				left: 50%;
				bottom: 1.95vh; 

				strong{
					margin-right: 0.2vw;
					padding-right: 0.5vw;
					font-weight: 700;
					position: relative;

					&::after{
						content: "";
						width: 1px;
						height: 1vh;
						background-color: #111;
						transform: translateY(-50%);
						position: absolute;
						right: 0; 
						top: 50%;
					}
				}
			}

			button{
				width: 15.62vh;
				height: 15.62vh;
				background-color: #ffffff70;
				z-index:3;
				@include text-hide;
			}
		}
	}

	//오디오북
	.audio-area{
		width: 100%;
		margin-top: 17.77vh;

		.one-book-area{
			margin: 0 auto;
			position: relative;

			&::before{
				content: "";
				height: 8vh;
				background-color: #CFCFCF;
				border-bottom-left-radius: 16px;
				border-bottom-right-radius: 25px;
				overflow: hidden;
				position: absolute;
				left: 1.25vh; 
				right: 1.35vh;
				bottom: 3.07vh;
				z-index: 2;
			}

			&::after{
				content: "";
				height: 8vh;
				background-color: #DEDEDE;
				border-bottom: 0.96vh solid #fff;
				border-bottom-left-radius: 16px;
				border-bottom-right-radius: 25px;
				overflow: hidden;
				position: absolute;
				left: 1.25vh; 
				right: 1.35vh;
				bottom: 1.25vh;
			}
		}

		.audio-bar-area{
			display: flex;
			justify-content: center;
			align-items: center;
			gap: 1.46vw;
			margin-top: 10.4492vh;

			.time{
				font-family: 'Binggrae';
				font-size: 2.3vh;
				color: #fff;
				font-weight: 700;
			}
		}

		.audio-bar{
			width: 57.1vw;
			height: 0.9766vh;
			background-color: #fff;
			border-radius: 10px;
			overflow: hidden;

			span{
				display: inline-block;
				height: 0.9766vh;
				background-color: #09A0FF;
				border-radius: 10px;
				vertical-align: top;
			}
		}

		.btn-control-area{
			display: flex;
			justify-content: center;
			gap: 0.8vw;
			margin-top: 8.39vh;

			button{
				display: flex;
				justify-content: center;
				align-items: center;
				width: 7.81vh;
				height: 7.81vh;
				background-image: none;
				background-color: #09A0FF;
				box-shadow: 0px -3.6px 5.4px 0px #14102780 inset, 0px 4px 6px 0px #FFFFFF40 inset, 0px 8px 16px 0px #00000029;
				transform: translateY(0);
				position: static;
				@include text-hide;

				&.play{
					&::before{
						content: "";
						width: 3.32vh;
						height: 3.32vh;
						background-image:url($btn-play);
						background-size: 100% 100%;
					}
				}

				&.pause{
					&::before{
						content: "";
						width: 2.92vh;
						height: 3.32vh;
						background-image:url($btn-pause);
						background-size: 100% 100%;
					}
				}

				&.hide{
					display: none;
				}
			}
		}
	}

	//문제풀기
	&.question{
		width: 100%;
		justify-content: flex-start;
		position: relative;

		.btn-ox-area{
			display: flex;
			align-items: center;
			justify-content: center;
			gap: 7.32vw;
			margin-top: 5.37vh; 
	
			.btn-ox{
				display: block;
				width: 34.17vh;
				height: 34.17vh;
				background-color: #FFCD1E;
				box-shadow: -20px -20px 30px 0px #F6980A inset, 20px 20px 20px 0px #FFFFFF80 inset, 0px 8px 16px 0px #00000029;
				border-radius: 100px;
				transform: translateY(0);
				position: relative;
				top: 0;
				@include text-hide;

				&::before{
					content: "";
					transform: translate(-50%, -50%);
					position: absolute;
					left: 50%;
					top: 50%;
				}

				&.select-o{
					&::before{
						width: 17.57vh;
						height: 17.57vh;
						background-image: url($select-o);
						background-size: 100%;
						
					}

					&.on{
						background-color: #09A0FF;
						box-shadow: -20px -20px 30px 0px #007DCC inset, 20px 20px 20px 0px #FFFFFF40 inset, 0px 8px 16px 0px #00000029;

						&::before{
							background-image: url($select-o-on);
						}
					}
				}

				&.select-x{
					&::before{
						width: 15vh;
						height: 15vh;
						background-image: url($select-x);
						background-size: 100%;
					}

					&.on{
						background-color: #09A0FF;
						box-shadow: -20px -20px 30px 0px #007DCC inset, 20px 20px 20px 0px #FFFFFF40 inset, 0px 8px 16px 0px #00000029;

						&::before{
							background-image: url($select-x-on);
						}
					}
				}

				&.correct{
					background-color: #09A0FF;
					box-shadow: -20px -20px 30px 0px #007DCC inset, 20px 20px 20px 0px #FFFFFF40 inset, 0px 8px 16px 0px #00000029;

					&.select-o{
						&::before{
							background-image: url($select-o-on);
							
						}
					}
	
					&.select-x{
						&::before{
							background-image: url($select-x-on);
						}
					}

					&::after{
						content: "";
						width: 34.17vh;
						height: 34.17vh;
						border: 20px solid #ED4043;
						border-radius: 100px;
						position: absolute;
						left: 0;
						top: 0;
					}
				}

				&:active{
					box-shadow: 20px 20px 30px 0px #F6980A inset, -20px -20px 20px 0px #FFFFFF80 inset, 0px 8px 16px 0px #00000029;
				}
			}
		}
	
		.study-complete-area{
			display: flex;
			justify-content: center;
			align-items: center;
			gap: 0.73vw;
			position: absolute; 
			left: 0;
			right: 0;
			bottom: 7.5vh;
	
			span{
				display: block;
				width: 1.95vh;
				height: 1.95vh;
				background-color: #fff;
				border-radius: 100%;
				@include text-hide;
	
				&.on{
					background-color: #089FFF;
				}
			}
		}

		.q-check-btn{
			display: flex;
			gap: 1.17vw;
			align-items: center;
			justify-content: center;
			position: absolute;
			left: 0;
			right: 0;
			bottom: 0.96vh;

			button{
				width: auto;
				height: 5.76vh;
				padding: 0 2.92vw;
				background-color: #FF6B00;
				box-shadow: -5px -5px 8px 0px #E9460080 inset, 0px 4px 6px 0px #FFFFFF66 inset, 0px 8px 16px 0px #FFFFFF29;
				border-radius: 100px;
				font-family: 'Binggrae';
				font-size: 2.69vh;
				color: #fff;
				line-height: 5.76vh;
				font-weight: 700;
				text-align: center;
				transform: translateY(0);
				position: static;

				&.btn-next {
					background-color: #09A0FF;
					box-shadow: -5px -5px 8px 0px #0072BA inset, 0px 4px 6px 0px #FFFFFF66 inset, 0px 8px 16px 0px #FFFFFF29;
				}
			}
			&.disabled{
				display: none;
			}
		}

		.layer-correct{
			width: 24vh;
			height: 24vh;
			background-image: url("../img/app/answer_o_q.png");
			border-radius: 100%;
			background-size: 100% 100%;
			transform: translate(-50%, -50%);
