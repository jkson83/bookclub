            z-index: 5;
            left: -30%;
            transform: translateY(50px) translateX(-50%);
            opacity: 0.7;
            filter: blur(3px);
    
            .postscript{
              transform: scale(.95);
              padding: 12rem 2rem 0;
    
              &::before{
                top: 3.5rem;
                width: 9.6rem;
                height: 9.6rem;
                transform: scale(1) translateX(-50%);
                background-size: contain;
              }
              .user-info{
                font-size: 1.2rem;
              }
              p:not(.user-info){
                margin: 3.5rem 0;
                font-size: 1.4rem;
                line-height: 1.4;
              }
            }
          }
          .selected{
            width: calc(100% - 4.8rem);
            z-index: 10;
            left: 50%;
            transform: translateY(0px) translateX(-50%);
    
            .postscript{
              left: 50%;
              transform: translateX(-50%);
              width: 62.5vw;
              height: 46.5vh;
              padding: 20rem 4rem 4rem;
              
              .user-info{
                font-size: 1.2rem;
              }
              p:not(.user-info){
                margin: 3rem 0;
                font-size: 1.4rem;
              }
            }
          }
          .next{
            z-index: 5;
            left: 60%;
            transform: translateY(50px) translateX(-50%);
            opacity: 0.7;
            filter: blur(3px);
            
            .postscript{
              transform: scale(.95);
              padding: 12rem 2rem 0;
    
              &::before{
                top: 3.5rem;
                width: 9.6rem;
                height: 9.6rem;
                transform: scale(1) translateX(-50%);
                background-size: contain;
              }
              .user-info{
                font-size: .8rem;
              }
              p:not(.user-info){
                margin: 3.5rem 0;
                font-size: 1.6rem;
                line-height: 1.4;
              }
            }
          }
        }
        .carousel-buttons{
          width: calc(100% - 1rem);
          height: 5rem;
          margin-top: 3rem;

          &-next, &-prev{
            width: 5rem;
            height: 5rem;
          }
        }
      }
