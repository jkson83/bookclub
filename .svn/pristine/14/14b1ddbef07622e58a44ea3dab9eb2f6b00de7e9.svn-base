      .postscript{
        position: relative;
        width: 68vw;
        height: 36vh;
        padding: 15rem 4rem 4rem;
        text-align: center;
        background-color: #fff;
        transition: width 400ms;
        -webkit-user-drag: none;
        border-radius: 4.68rem;
  
        &::before{
          content: '';
          position: absolute;
          top: 6.4rem; //-14rem
          left: 50%;
          width: 12rem;  //176px;
          height: 12rem;  //175px;
          background: url('../../resources/img/web/main/mo/book_v6_man.png') 50% no-repeat;
          transform: translateX(-50%);
          background-size: contain;
        }
        p:not(.user-info){
          overflow: hidden;
          display: -webkit-box;
          -webkit-box-orient: vertical;
          -webkit-line-clamp: 3;
          margin: 2.5rem 0;
          color: #111;
          font-size: 1.375rem;
          font-weight: 700;
          line-height: 1.9rem;
          text-overflow: ellipsis;
          word-break: keep-all;
        }
        .user-info{
          color: #666;
          font-family: 'binggrae';
          font-size: 1.8rem;
  
